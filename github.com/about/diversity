              data-analytics-event="{&quot;category&quot;:&quot;Diversity Hero section&quot;,&quot;action&quot;:&quot;click to scroll to content&quot;,&quot;label&quot;:&quot;ref_page:/about/diversity;ref_cta:People;ref_loc:diversity hero section&quot;}"
            >
              People
